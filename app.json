{
  "expo": {
    "name": "JLTCB",
    "slug": "JLTCB",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "jltcb",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.jltcb.jltcbapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/appImage.png",
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-video",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0bc8ca02-845f-4252-a676-51c678e10146"
      }
    }
  }
}
