{
  "expo": {
    "name": "JLTCB",
    "slug": "JLTCB",
    "version": "2.1.0",
    "orientation": "portrait",
    "scheme": "jltcb",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jltcb.jltcbapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.jltcb.jltcbapp",
      "versionCode": 2,
      "permissions": ["INTERNET"],
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/appImage.png",
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-video",
      "expo-web-browser",
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/martel-sans/900Black/MartelSans_900Black.ttf",
            "node_modules/@expo-google-fonts/martel-sans/800ExtraBold/MartelSans_800ExtraBold.ttf"
          ]
        }
      ],
      "expo-secure-store",
      [
        "expo-asset",
        {
          "assets": ["./src/assets/dashboard-folder-icons"]
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0bc8ca02-845f-4252-a676-51c678e10146"
      }
    }
  }
}
